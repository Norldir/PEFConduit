																</xs:annotation>
															</xs:element>
															<xs:element name="status" type="tpStatusCartao">
																<xs:annotation>
																	<xs:documentation>Status do cartão do condutor para a gestora de cartões informada.</xs:documentation>
																</xs:annotation>
															</xs:element>
														</xs:sequence>
													</xs:complexType>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:choice>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
