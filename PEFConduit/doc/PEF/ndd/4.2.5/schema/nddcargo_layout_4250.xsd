								<xs:sequence>
									<xs:element name="ajusteParcela" type="tpAjusteValoresParcela" maxOccurs="unbounded"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="observacao" minOccurs="0">
							<xs:simpleType>
								<xs:restriction base="tpString">
									<xs:minLength value="1"/>
									<xs:maxLength value="500"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="ID" type="tpGUID" use="optional">
						<xs:annotation>
							<xs:documentation>Identificador único para a confirmação de pagamento.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="Id" type="tpGUID" use="optional">
						<xs:annotation>
							<xs:documentation>Identificador único para a confirmação de pagamento.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element ref="ds:Signature"/>
		</xs:sequence>
		<xs:attribute name="versao" type="tpVersao" use="required">
			<xs:annotation>
				<xs:documentation>Versão do documento.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="token" type="tpToken" use="required">
			<xs:annotation>
				<xs:documentation>Token de processamento.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
