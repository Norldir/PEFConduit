						<xs:element name="mfParcelas" type="tpSimNao">
							<xs:annotation>
								<xs:documentation>Indica se a operação de transporte possui ou não movimentação financeira de parcelas. 1 = Sim e 2 = Não.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="mfPedagio" type="tpPagamentoPedagio">
							<xs:annotation>
								<xs:documentation>Determina a configuração de pagamento do pedágio. 1 = Com movimentação financeira em cartão, 2 = Sem movimentação financeira em cartão ou 3 = Via Fácil.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="emAlteracao" type="tpSimNao">
							<xs:annotation>
								<xs:documentation>Indica se a operação de transporte está em alteração. 1 = Sim e 2 = Não.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="status" type="tpStatusOT">
							<xs:annotation>
								<xs:documentation>Indica o status da operação de transporte. Vigente = 0, Encerrada = 1, Cancelada = 2, Aguardando Vigência = 4, Aberta = 5, Pendente = 6, Em Contingência = 7 ou Rejeitada = 8</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="gestoraCartao" type="tpGestoraCartao" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Indica a gestora de cartões da operação de transporte.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="autorizacao" type="tpAutorizacaoOT"/>
			<xs:element name="avisoTransportador" type="tpString" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Aviso ao transportador disponibilizado pela ANTT ou Administradora de meios de pagamento</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="valores" type="tpRetornoValores"/>
			<xs:choice minOccurs="0">
				<xs:element name="pedagio">
					<xs:annotation>
						<xs:documentation>Dados refetentes ao pedágio.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="valor" type="tpDec_1302">
								<xs:annotation>
									<xs:documentation>Valor total do pedágio.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="totalPago" type="tpDec_1302">
								<xs:annotation>
									<xs:documentation>Valor total de pagamento já realizado.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="saldoPagar" type="tpDec_1302">
								<xs:annotation>
									<xs:documentation>Saldo restante a pagar.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="origem" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Cidade e estado de origem.</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="tpString">
										<xs:minLength value="1"/>
										<xs:maxLength value="200"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="destino" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Cidade e estado de destino.</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="tpString">
										<xs:minLength value="1"/>
										<xs:maxLength value="200"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="direcionamentosPedagio" type="tpResumoDirecionamentosPedagio">
					<xs:annotation>
						<xs:documentation>Informações de pedágio para operações de transporte com direcionamento de pedágio.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:element name="pedagiosViaFacil" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Recibo dos pedágios comprados no Via Fácil.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="reciboViagem" type="tpInfRetornoConsultaReciboViagemViaFacilDetalhe" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="parcelas" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="parcela" maxOccurs="unbounded">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="nome">
										<xs:annotation>
											<xs:documentation>Nome para apresentação da parcela dentro do sistema.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="tpString">
												<xs:minLength value="1"/>
												<xs:maxLength value="50"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="valorReal" type="tpDec_1302">
										<xs:annotation>
											<xs:documentation>Valor real para pagamento da parcela.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="valorAplicado" type="tpDec_1302">
										<xs:annotation>
											<xs:documentation>Valor aplicado para a parcela. Não levando em consideração os descontos.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="dataPrevisao" type="tpData">
										<xs:annotation>
											<xs:documentation>Data prevista para o pagamento. Padrão AAAA-MM-DD</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="dataPagamento" type="tpDataHora" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Data e hora de efetivação do pagamento.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="status" type="tpStatusParcela">
										<xs:annotation>
											<xs:documentation>Indica o status da parcela. 0 = Em aberto, 1 = Aguardando pagamento, 2 = Aguardando solicitação de pagamento, 3 = Paga ou 4 = Cancelada</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="efetivacao" type="tpEfetivacaoPagamentoRetorno">
										<xs:annotation>
											<xs:documentation>Define o local de efetivação do pagamento da parcela. Possibilidades: 1 = Posto credenciado, 2 = Centro de Triagem, 3 = Contratante, 4 = Confirmador, 5 Pagamento Imediato ou 6 = Agendado</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="prazoMinimo" type="tpPrazoMinimo">
										<xs:annotation>
											<xs:documentation>Define a configuração de prazo mínimo para pagamento. 1 = não utilizar, 2 = baseado em dia fixo configurado na contratante ou 3 = baseado em data prevista.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="confirmarPgto" type="tpSimNao">
										<xs:annotation>
											<xs:documentation>Define se a contratante precisa ou não confirmar o pagamento da parcela após o evento de efetivação acontecer. 1 = sim ou 2 = não.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="carga" type="tpCargaParcela" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Caga utilizada para o desconto por mercadoria entregue.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="transferenciaAutomatica" type="tpNovaParcelaTransferenciaAutomatica" minOccurs="0"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="condutores" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Dados dos condutores</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="condutor" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Dados do condutor</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="cpf" type="tpCpf">
										<xs:annotation>
											<xs:documentation>Número do CPF do condutor.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="nome">
										<xs:annotation>
											<xs:documentation>Nome do condutor.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="tpString">
												<xs:minLength value="1"/>
												<xs:maxLength value="150"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!--Consulta de saldo cartão frota-->
	<xs:complexType name="tpConsultaSaldoCartaoFrota">
		<xs:annotation>
			<xs:documentation>Consulta do resumo da operação de transporte.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="infLote">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="cnpj" type="tpCnpj">
							<xs:annotation>
								<xs:documentation>Cnpj da contratante.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="gestoraCartao" type="tpGestoraCartao">
							<xs:annotation>
								<xs:documentation>Gestora de cartão dos cartões que serão consultados.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ide" type="tpInfConsultaSaldoCartaoFrota" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
					<xs:attribute name="ID" type="tpGUID">
						<xs:annotation>
							<xs:documentation>Identificador único para a consulta de operações de transporte.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="Id" type="tpGUID">
						<xs:annotation>
							<xs:documentation>Identificador único para a consulta de operações de transporte.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element ref="ds:Signature"/>
		</xs:sequence>
		<xs:attribute name="versao" type="tpVersao" use="required">
			<xs:annotation>
				<xs:documentation>Versão do documento.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="token" type="tpToken" use="required">
			<xs:annotation>
				<xs:documentation>Token de processamento.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="tpInfConsultaSaldoCartaoFrota">
		<xs:annotation>
			<xs:documentation>Informações da consulta do resumo da operação de transporte.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice>
				<xs:element name="cpfColaborador" type="tpCpf">
					<xs:annotation>
						<xs:documentation>CPF do colaborador.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="cartaoId">
					<xs:annotation>
						<xs:documentation>Identificador do cartão.</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:long">
							<xs:minInclusive value="1"/>
							<xs:maxInclusive value="1000000000000000"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="tpRetornoConsultaSaldoCartaoFrota">
		<xs:annotation>
			<xs:documentation>Retorno da consulta do resumo da operação de transporte.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="mensagens" type="tpMensagens" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Lista das mensagens de erro.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="envioConsulta">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="infLote">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="cnpjContratante" type="tpCnpj">
										<xs:annotation>
											<xs:documentation>Cnpj da contratante.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="gestoraCartao" type="tpGestoraCartao">
										<xs:annotation>
											<xs:documentation>Gestora de cartão.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="ide" type="tpInfConsultaSaldoCartaoFrota" minOccurs="0" maxOccurs="unbounded"/>
								</xs:sequence>
								<xs:attribute name="versao" type="tpVersao" use="required">
									<xs:annotation>
										<xs:documentation>Versão do documento.</xs:documentation>
									</xs:annotation>
								</xs:attribute>
								<xs:attribute name="token" type="tpToken" use="required">
									<xs:annotation>
										<xs:documentation>Token de processamento.</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="retornoConsulta" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="infConsulta" maxOccurs="unbounded">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="mensagem" type="tpMensagem">
										<xs:annotation>
											<xs:documentation>Observação da consulta.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="nomeColaborador" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Nome do colaborador vinculado ao cartão.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="tpString">
												<xs:minLength value="1"/>
												<xs:maxLength value="150"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="cpfColaborador" type="tpCpf" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Cpf do colaborador vinculado ao cartão.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="dataEntregaCartao" type="tpDataHora" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Data que foi entregue o cartão para o colaborador.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="cartaoId" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Identificador do cartão.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:long">
												<xs:minInclusive value="1"/>
												<xs:maxInclusive value="1000000000000000"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="gestoraCartao" type="tpGestoraCartao" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Gestora de cartão do cartão do cartão.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="saldo" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Informações do cartão ativo do colaborador.</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element name="saldoPedagioDisponivel" type="tpDec_1302">
													<xs:annotation>
														<xs:documentation>Valor de pedágio disponível para carga.</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="saldoDisponivel" type="tpDec_1302">
													<xs:annotation>
														<xs:documentation>Saldo disponível no cartão.</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="dataConsulta" type="tpDataHora">
													<xs:annotation>
														<xs:documentation>Data que foi realizada a consulta.</xs:documentation>
													</xs:annotation>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!--Operações de pagamento-->
	<xs:complexType name="tpOperacaoPagamento">
		<xs:annotation>
			<xs:documentation>Consulta do resumo da operação de transporte.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="infOperacaoPagamento" type="tpInfOperacaoPagamento">
				<xs:annotation>
					<xs:documentation>Informações da operação de pagamento.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="ds:Signature"/>
		</xs:sequence>
		<xs:attribute name="versao" type="tpVersao" use="required">
			<xs:annotation>
				<xs:documentation>Versão do documento.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="token" type="tpToken" use="required">
			<xs:annotation>
				<xs:documentation>Token de processamento.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="tpInfOperacaoPagamento">
		<xs:annotation>
			<xs:documentation>Consulta do resumo da operação de pagametno.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ide">
				<xs:annotation>
					<xs:documentation>Informações da contratante que esta realizando o pagamento.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="cnpj" type="tpCnpj">
							<xs:annotation>
								<xs:documentation>Cnpj da contratante que esta realizando o pagamento.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ptEmissor">
							<xs:annotation>
								<xs:documentation>Ponto emissor que esta realizando o pagamento.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="tpString">
									<xs:minLength value="1"/>
									<xs:maxLength value="30"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="numero" type="tpNumeroOT">
							<xs:annotation>
								<xs:documentation>Número para a operação de pagamento.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="serie" type="tpSerieOT">
							<xs:annotation>
								<xs:documentation>Série da operação de pagamento.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="colaborador">
				<xs:annotation>
					<xs:documentation>Informações do colaborador que esta recebendo o pagamento.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="cpf" type="tpCpf">
							<xs:annotation>
								<xs:documentation>Número do Cpf do colaborador.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="informacoes" type="tpColaborador" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Informações para cadastro de colaborador.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="cartao" type="tpCartaoObrigatorioColaborador" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Informações para vinculo de cartão com o colaborador.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="rota" type="tpRota" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Define a rota da viagem.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="pagamento">
				<xs:annotation>
					<xs:documentation>Informações sobre o pagamento.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="gestoraCartao" type="tpGestoraCartao">
							<xs:annotation>
								<xs:documentation>Gestora de cartão do cartão ativo do colaborador.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="valor" type="tpDec_1302" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Valor que esta sendo pago. Este campo será considerado obrigatório caso não seja informado o campo valorPedagio.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="valorPedagio" type="tpDec_1302" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Valor que esta sendo pago como pedágio.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="categoriaPedagio" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Se a rota estiver utilizando o  roteirizador é necessário informar qual a categoria para calculo de pedágio. Valor padrão é 0 = Isento</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="tpCategoriaPedagio">
						<xs:minInclusive value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="ID" type="tpGUID">
			<xs:annotation>
				<xs:documentation>Identificador único para a consulta de operações de transporte.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Id" type="tpGUID">
			<xs:annotation>
				<xs:documentation>Identificador único para a consulta de operações de transporte.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="impAuto" type="tpSimNao">
			<xs:annotation>
				<xs:documentation>Determina se deverá ser impresso automaticamente o pagamento. 1 = Sim e 2 = Não. Por padrão será impressa automaticamente.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="tpInfRetornoOperacaoPagamento">
		<xs:annotation>
			<xs:documentation>Consulta do resumo da operação de pagametno.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ide">
				<xs:annotation>
					<xs:documentation>Informações da contratante que esta realizando o pagamento.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
