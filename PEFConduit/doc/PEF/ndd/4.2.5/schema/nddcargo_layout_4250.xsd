	<xs:complexType name="tpNovaParcelaTransferenciaAutomatica">
		<xs:annotation>
			<xs:documentation>Define os dados de transferência automática para uma nova parcela.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="cpfCondutor" type="tpCpf">
				<xs:annotation>
					<xs:documentation>Número do CPF do condutor beneficiado com a transferência automática.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="finalidadeParcela" type="tpFinalidadeParcela">
				<xs:annotation>
					<xs:documentation>Define a finalidade da parcela para uso na transferência automática de condutores. 1 = adiantamento ou 2 = saldo.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="tpRemoverParcelatransferenciaAutomatica">
