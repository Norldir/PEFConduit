						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element ref="ds:Signature"/>
		</xs:sequence>
		<xs:attribute name="versao" type="tpVersao" use="required">
			<xs:annotation>
				<xs:documentation>Versão do documento.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
