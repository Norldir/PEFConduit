							<xs:annotation>
								<xs:documentation>Nome da parcela.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="tpString">
									<xs:minLength value="1"/>
									<xs:maxLength value="50"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="prazoMinimo" minOccurs="0">
