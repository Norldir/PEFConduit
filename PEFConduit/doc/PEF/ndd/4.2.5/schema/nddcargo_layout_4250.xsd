		<xs:sequence>
			<xs:element name="infConsultaPgtos">
				<xs:complexType>
					<xs:sequence>
						<xs:choice>
							<xs:element name="cnpjContratante" type="tpCnpj">
								<xs:annotation>
									<xs:documentation>CNPJ da contratante ou agrupador solicitante dos pagamentos.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="cnpjEmitente" type="tpCnpj">
								<xs:annotation>
									<xs:documentation>CNPJ do emitente solicitante dos pagamentos.</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:choice>
						<xs:choice>
							<xs:element name="GUID" type="tpGUID">
								<xs:annotation>
									<xs:documentation>Identificador único a ser associado com a integração de pagamentos. Caso ja existe uma integração para este identificador, esta será retornada.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="periodo">
								<xs:annotation>
									<xs:documentation>Intervalo de datas a ser retornado os pagamentos.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="dataInicial" type="tpData">
											<xs:annotation>
												<xs:documentation>Data inicial para a conuslta de pagamentos.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="dataFinal" type="tpData">
											<xs:annotation>
												<xs:documentation>Data final para a consulta de pagamentos.</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:choice>
					</xs:sequence>
					<xs:attribute name="ID" type="tpGUID">
						<xs:annotation>
							<xs:documentation>Identificador único para a consulta de pagamentos reslizados.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="Id" type="tpGUID">
						<xs:annotation>
							<xs:documentation>Identificador único para a consulta de pagamentos realizados.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element ref="ds:Signature"/>
		</xs:sequence>
		<xs:attribute name="versao" type="tpVersao" use="required">
			<xs:annotation>
				<xs:documentation>Versão do documento.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="token" type="tpToken" use="required">
			<xs:annotation>
				<xs:documentation>Token de processamento.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="tpRetornoConsultaPgtos">
		<xs:annotation>
			<xs:documentation>Retorno das consultas de pagementos realizados.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="mensagens" type="tpMensagens" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Lista das mensagens de erro.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="pgtos" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Pagamentos realizados.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="pgto" type="tpPgto" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="tpPgto">
		<xs:annotation>
			<xs:documentation>Dados de pagamento realizado.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="autorizacao">
				<xs:annotation>
					<xs:documentation>Identificação da operação de transporte.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:choice>
						<xs:element name="ciot">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="numero" type="tpCIOT">
										<xs:annotation>
											<xs:documentation>Código de identificação da operação de transporte.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="ciotCodVerificador" type="tpCIOTCodVerificador">
										<xs:annotation>
											<xs:documentation>Protocolo de autorização disponibilizado pela ANTT. Será informado XXXX caso seja uma emissão em contingência.</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="ndot">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="numero" type="tpCIOT">
										<xs:annotation>
											<xs:documentation>Número que identifica a operação de transporte sem CIOT.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="ndotCodVerificador" type="tpCIOTCodVerificador">
										<xs:annotation>
											<xs:documentation>Protocolo de autorização disponibilizado para o NDOT.</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="ndvp">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="numero" type="tpCIOT">
										<xs:annotation>
											<xs:documentation>Número que identifica a operação de vale pedágio.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="ndvpCodVerificador" type="tpCIOTCodVerificador">
										<xs:annotation>
											<xs:documentation>Protocolo de autorização disponibilizado para o NDVP.</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:choice>
				</xs:complexType>
			</xs:element>
			<xs:element name="dataHora" type="tpDataHora">
				<xs:annotation>
					<xs:documentation>Data e hora que o pagamento foi realizado.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:choice>
				<xs:element name="pedagio">
					<xs:annotation>
						<xs:documentation>Pagamento específico de pedágio.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="valorPago" type="tpDec_1302">
								<xs:annotation>
									<xs:documentation>Valor total do pagamento de pedágio.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="estabelecimento">
								<xs:annotation>
									<xs:documentation>Estabelecimento onde foi pago o valor do pedágio.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="cnpj" type="tpCnpj">
											<xs:annotation>
												<xs:documentation>CNPJ do estabelecimento.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="nome">
											<xs:annotation>
												<xs:documentation>Nome do estabelecimento.</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="tpString">
													<xs:minLength value="1"/>
													<xs:maxLength value="150"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="endereco" type="tpEnderecoSemIBGE">
											<xs:annotation>
												<xs:documentation>Endereço do estabelecimento.</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="cpfRecebedor" type="tpCpf">
								<xs:annotation>
									<xs:documentation>Número do CPF do condutor/colaborador que recebeu a carga do pedágio.</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="frete">
					<xs:annotation>
						<xs:documentation>Pagamento específico do frete, levando em consideração o combustível e também as despesas.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="parcelas">
								<xs:annotation>
									<xs:documentation>Parcelas do CIOT envolvidas no pagamento.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="parcela" maxOccurs="unbounded">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="nome">
														<xs:annotation>
															<xs:documentation>Nome da parcela.</xs:documentation>
														</xs:annotation>
														<xs:simpleType>
															<xs:restriction base="tpString">
																<xs:minLength value="1"/>
																<xs:maxLength value="50"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="valorPago" type="tpDec_1302">
														<xs:annotation>
															<xs:documentation>Valor do pagamento realizado</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="efetivacao">
														<xs:annotation>
															<xs:documentation>Local da efetivação do pagamento.</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:choice>
																	<xs:element name="postoCredenciado" type="tpRetornoPessoaJuridica">
																		<xs:annotation>
																			<xs:documentation>Pagamento realizado devido a um registro de passagem no posto credenciado.</xs:documentation>
																		</xs:annotation>
																	</xs:element>
																	<xs:element name="centroTriagem" type="tpRetornoPessoa">
																		<xs:annotation>
																			<xs:documentation>Pagamento realizado devido a uma triagem de coleta com sucesso no centro de triagem.</xs:documentation>
																		</xs:annotation>
																	</xs:element>
																	<xs:element name="contratante" type="tpRetornoPessoaJuridica">
																		<xs:annotation>
																			<xs:documentation>Pagamento realizado devido a uma triagem de coleta com sucesso na contratante.</xs:documentation>
																		</xs:annotation>
																	</xs:element>
																	<xs:element name="confirmacaoEletronica" type="tpRetornoPessoa">
																		<xs:annotation>
																			<xs:documentation>Pagamento realizado devido a uma confirmação eletrônica de recebimento da carga.</xs:documentation>
																		</xs:annotation>
																	</xs:element>
																	<xs:element name="automatica">
																		<xs:annotation>
																			<xs:documentation>Pagamento realizado devido a um agendamento.</xs:documentation>
																		</xs:annotation>
																		<xs:complexType>
																			<xs:sequence>
																				<xs:element name="dataHora" type="tpDataHora">
																					<xs:annotation>
																						<xs:documentation>Data e hora do agendamento.</xs:documentation>
																					</xs:annotation>
																				</xs:element>
																			</xs:sequence>
																		</xs:complexType>
																	</xs:element>
																</xs:choice>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="comentario" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Comentário realizado durante a ação que deu origem ao pagamento.</xs:documentation>
														</xs:annotation>
														<xs:simpleType>
															<xs:restriction base="tpString">
																<xs:minLength value="1"/>
																<xs:maxLength value="500"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="carga" minOccurs="0">
														<xs:complexType>
															<xs:sequence>
																<xs:element name="saida" type="tpCargaParcela"/>
																<xs:element name="entrega" type="tpCargaParcelaEntregaComPostoCredenciado" minOccurs="0"/>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="confirmacaoPgto" minOccurs="0">
														<xs:complexType>
															<xs:sequence>
																<xs:element name="dataHoraConfirmacao" type="tpDataHora">
																	<xs:annotation>
																		<xs:documentation>Data e hora em que foi realizada a confirmação do pagamento pela contratante. Padrão: AAAA-MM-DD HH:MM:SS</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="observacao" minOccurs="0">
																	<xs:simpleType>
																		<xs:restriction base="tpString">
																			<xs:minLength value="1"/>
																			<xs:maxLength value="500"/>
																		</xs:restriction>
																	</xs:simpleType>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<!--Solicitação de impressão da operação de transporte-->
	<xs:complexType name="tpSolicitarImpressao">
		<xs:annotation>
			<xs:documentation>Solicitar a impressão de uma operação de transporte.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="cnpj" type="tpCnpj">
				<xs:annotation>
					<xs:documentation>CNPJ da contratante.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="autorizacao" type="tpAutorizacaoOT">
				<xs:annotation>
					<xs:documentation>Operação de transporte.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ptEmissor">
				<xs:annotation>
					<xs:documentation>Ponto emissor para realizar a impressão.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="tpString">
						<xs:minLength value="1"/>
						<xs:maxLength value="30"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="versao" type="tpVersao" use="required">
			<xs:annotation>
				<xs:documentation>Versão do documento.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="token" type="tpToken" use="required">
			<xs:annotation>
				<xs:documentation>Token de processamento.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<!--Cross Talk Message-->
	<xs:complexType name="CrossTalk_Header_Response_Type">
		<xs:annotation>
			<xs:documentation>Cabeçalho da mensagem</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ResponseCode" type="xs:int">
				<xs:annotation>
					<xs:documentation>Codigo da resposta do processamento da mensagem</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ResponseCodeMessage" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Descricao do codigo da mensagem</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ProcessCode" type="xs:int">
				<xs:annotation>
					<xs:documentation>Código do processamento</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="MessageType" type="xs:int">
				<xs:annotation>
					<xs:documentation>Tipo de mensagem que será processada</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ExchangePattern" type="xs:byte" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Padrão de troca de mensagem</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SourceId" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Identifdicador do sistema que está ntegrando</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:byte">
						<xs:pattern value="0|1|2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="GUID" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Identificador unico gerado pela aplicação para poder recuperar a mensagem </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DateTime" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Data e hora do envio da mensagem para processamento</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EnterpriseId" type="xs:long">
				<xs:annotation>
					<xs:documentation>Identificador da empresa</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Token" type="xs:string">
				<xs:annotation>
					<xs:documentation>Identificador exclusivo que contêm os vinculos de processamento necessários no ETransportBusiness</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ContentType" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Tipo de conteudo da mensagem enviada</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ContentEncoding" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Tipo de codificação dos caracteres passados na mensagem</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!--Encerramento de operação de transporte-->
	<xs:complexType name="tpEncerramentoOT">
		<xs:annotation>
			<xs:documentation>Encerramento de operação de transporte</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="infEnceOT" type="tpInfEnceOT">
				<xs:annotation>
					<xs:documentation>Informações referentes a operação de transporte a ser encerrada.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="ds:Signature"/>
		</xs:sequence>
		<xs:attribute name="versao" type="tpVersao" use="required">
			<xs:annotation>
				<xs:documentation>Versão do documento.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="token" type="tpToken" use="required">
			<xs:annotation>
				<xs:documentation>Token de processamento.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="tpInfEnceOT">
		<xs:annotation>
			<xs:documentation>Informações do encerramento de operação de transporte.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="cnpj" type="tpCnpj">
				<xs:annotation>
					<xs:documentation>CNPJ da contratante da operação que se deseja encerrar.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="autorizacao" type="tpAutorizacaoOT">
				<xs:annotation>
					<xs:documentation>Operação de transporte a ser encerrada.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="encerramento">
				<xs:annotation>
					<xs:documentation>Dados referentes ao encerramento.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:choice>
						<xs:element name="padrao">
							<xs:annotation>
								<xs:documentation>Operação de transporte padrão.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="qtdeCarga" type="tpDec_0502">
										<xs:annotation>
											<xs:documentation>Quantidade da carga em Kg.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="adicionais" type="tpAdicionalImpressao" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Dados adicionais para uso na impressão</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="TACagregado">
							<xs:annotation>
								<xs:documentation>Operação de transporte com TAC-agregado.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="viagens">
										<xs:annotation>
											<xs:documentation>Viagens realizadas durante o período de vigência da operação de transporte.</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element name="viagem" maxOccurs="unbounded">
													<xs:complexType>
														<xs:sequence>
															<xs:element name="rota" type="tpRota">
																<xs:annotation>
																	<xs:documentation>Define a rota da viagem.</xs:documentation>
																</xs:annotation>
															</xs:element>
															<xs:element name="codigoSH" type="tpNaturezaCarga">
																<xs:annotation>
																	<xs:documentation>Código do Sistema Harmonizado. Define qual a natureza da carga conforme a tabela de códigos do Sistema Harmonizado de Designação e de Codificação de Mercadorias. </xs:documentation>
																</xs:annotation>
															</xs:element>
															<xs:element name="qtdeCarga" type="tpDec_0802">
																<xs:annotation>
																	<xs:documentation>Informa a quantidade total da carga de todas as operações de transporte realizadas.</xs:documentation>
																</xs:annotation>
															</xs:element>
															<xs:element name="qtdeViagens" type="tpInt">
																<xs:annotation>
																	<xs:documentation>Quantidade de viagens realizadas durante o período de vigência da operação de transporte.</xs:documentation>
																</xs:annotation>
															</xs:element>
														</xs:sequence>
													</xs:complexType>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
									<xs:element name="adicionais" type="tpAdicionalImpressao" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Dados adicionais para uso na impressão</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:choice>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="ID" type="tpGUID" use="optional">
			<xs:annotation>
				<xs:documentation>Identificador unico para o encerramento</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Id" type="tpGUID" use="optional">
			<xs:annotation>
				<xs:documentation>Identificador unico para o encerramento</xs:documentation>
			</xs:annotation>
		</xs:attribute>
