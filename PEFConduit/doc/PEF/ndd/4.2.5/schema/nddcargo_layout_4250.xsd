										<xs:simpleType>
											<xs:restriction base="tpString">
												<xs:minLength value="1"/>
												<xs:maxLength value="50"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="nomeConcessionaria">
										<xs:annotation>
											<xs:documentation>Nome da concessionária onde se encontra a praça.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="tpString">
												<xs:minLength value="1"/>
												<xs:maxLength value="50"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="placaVeiculo">
										<xs:annotation>
											<xs:documentation>Placa do veículo que contém a tag do Via Fácil.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="1"/>
												<xs:maxLength value="7"/>
												<xs:pattern value="[a-zA-Z0-9]+"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="tag">
										<xs:annotation>
											<xs:documentation>Código identificador do tag ViaFacil do veículo.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="tpString">
												<xs:minLength value="1"/>
												<xs:maxLength value="50"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="valorTarifa" type="tpDec_1302">
										<xs:annotation>
											<xs:documentation>Valor da tarifa do pedágio na praça.</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!--Mensagem de retorno-->
	<xs:complexType name="tpMensagemErro">
		<xs:annotation>
			<xs:documentation>Lista das mensagens de erro.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="descricao" maxOccurs="unbounded">
				<xs:simpleType>
					<xs:restriction base="tpString">
						<xs:minLength value="1"/>
						<xs:maxLength value="200"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="tpRetornoPadrao">
		<xs:annotation>
			<xs:documentation>Retorno padrão.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="mensagem" type="tpMensagem"/>
		</xs:sequence>
		<xs:attribute name="versao" use="required">
			<xs:annotation>
				<xs:documentation>versão do documento</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="tpMensagem">
		<xs:annotation>
			<xs:documentation>Tipo complexo que contém o código a descrição de uma mensagem</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="categoria">
				<xs:annotation>
					<xs:documentation>Categoria da mensagem de retorno</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:int">
						<xs:pattern value="[0-9]{3}"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="codigo">
				<xs:annotation>
					<xs:documentation>Codigo da mensagem sendo retornada</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:int">
						<xs:pattern value="[0-9]{3,4}"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="mensagem">
				<xs:annotation>
					<xs:documentation>Descrição da mensagem</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="tpDescricaoMensagem">
						<xs:maxLength value="2000"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="observacao" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Observacao sobre  a mensagem. Nesta observação é que serão apresentados informações mais técnicas do que está acontecendo </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="2000"/>
						<xs:whiteSpace value="preserve"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="tpMensagens">
		<xs:annotation>
			<xs:documentation>Tipo complexo que contém as mensagens de retorno</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="mensagem" type="tpMensagem" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="tpColaborador">
		<xs:annotation>
			<xs:documentation>Informações sobre o colaborador.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="nome">
				<xs:annotation>
					<xs:documentation>Nome do colaborador.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="tpString">
						<xs:minLength value="1"/>
						<xs:maxLength value="150"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="identidade">
				<xs:annotation>
					<xs:documentation>Identidade do colaborador.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="tpString">
						<xs:minLength value="1"/>
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="endereco" type="tpEnderecoSemIBGE">
				<xs:annotation>
					<xs:documentation>Endereço do colaborador.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="telefone">
				<xs:annotation>
					<xs:documentation>Telefone celular do colaborador.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:long">
						<xs:minInclusive value="1000000000"/>
						<xs:maxInclusive value="100000000000"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="tpCartaoColaborador">
		<xs:annotation>
			<xs:documentation>Informações sobre o cartão do colaborador.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="cartaoId" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Este será o identificador do cartão que foi entregue ao colaborador e será realizado o vínculo.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:long">
						<xs:minInclusive value="1"/>
						<xs:maxInclusive value="1000000000000000"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="gestoraCartao" type="tpGestoraCartao" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Gestora de cartão do cartão entregue ao colaborador, no caso de vinculo será utilizada esta gestora de cartão para realizar o vinculo do cartão.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="tpCartaoObrigatorioColaborador">
		<xs:annotation>
			<xs:documentation>Informações sobre o cartão do colaborador.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="cartaoId">
				<xs:annotation>
					<xs:documentation>Este será o identificador do cartão que foi entregue ao colaborador e será realizado o vínculo.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:long">
						<xs:minInclusive value="1"/>
						<xs:maxInclusive value="1000000000000000"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="gestoraCartao" type="tpGestoraCartao">
				<xs:annotation>
					<xs:documentation>Gestora de cartão do cartão entregue ao colaborador, no caso de vinculo será utilizada esta gestora de cartão para realizar o vinculo do cartão.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="tpAutorizacaoImpressao">
		<xs:annotation>
			<xs:documentation>Informações relacionadas a autorização de uma operação de transporte.</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element name="ciot">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="numero" type="tpCIOT">
							<xs:annotation>
								<xs:documentation>Código de identificação da operação de transporte.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ciotCodVerificador" type="tpCIOTCodVerificador">
							<xs:annotation>
								<xs:documentation>Protocolo de autorização disponibilizado pela ANTT. Será informado XXXX caso seja uma emissão em contingência.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="ndot">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="numero" type="tpCIOT">
							<xs:annotation>
								<xs:documentation>Número que identifica a operação de transporte sem CIOT.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ndotCodVerificador" type="tpCIOTCodVerificador">
							<xs:annotation>
								<xs:documentation>Protocolo de autorização disponibilizado para o NDOT.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="ndop">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="numero" type="tpCIOT">
							<xs:annotation>
								<xs:documentation>Número que identifica a operação de pagamento.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ndopCodVerificador" type="tpNDOPCodVerificador">
							<xs:annotation>
								<xs:documentation>Protocolo de autorização disponibilizado para o NDOP.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="ndvp">
				<xs:annotation>
					<xs:documentation>Número do documento de vale pedágio.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="numero" type="tpCIOT">
							<xs:annotation>
								<xs:documentation>Código de identificação da operação de vale pedágio.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ndvpCodVerificador" type="tpNDVPCodVerificador">
							<xs:annotation>
								<xs:documentation>Protocolo de autorização.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<!--Operação de transporte-->
	<xs:complexType name="tpOT">
		<xs:annotation>
			<xs:documentation>Operação de transporte.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="infOT">
				<xs:annotation>
					<xs:documentation>Informações sobre a operação de transporte.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="ide">
							<xs:annotation>
								<xs:documentation>Dados de identificação da operação de transporte.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="cnpj" type="tpCnpj">
										<xs:annotation>
											<xs:documentation>Cnpj da contratante responsável pela operação de transporte.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="numero" type="tpNumeroOT">
										<xs:annotation>
											<xs:documentation>Número para a operação de transporte.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="serie" type="tpSerieOT">
										<xs:annotation>
											<xs:documentation>Série da operação de transporte.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="ptEmissor">
										<xs:annotation>
											<xs:documentation>Ponto emissor responsável pela operação de transporte.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="tpString">
												<xs:minLength value="1"/>
												<xs:maxLength value="30"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="dtInicio" type="tpData" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Data de inicio da viagem. Não deve ser informada pra OT com TAC_agregado. Padrão: AAAA-MM-DD</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="dtFim" type="tpData" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Data de término da viagem. Se não for informada, será definido a data final como 30 dias após a data de início. Padrão: AAAA-MM-DD</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="contrato" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Informar o número do contrato entre o transportador e a contratante.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="tpString">
												<xs:minLength value="1"/>
												<xs:maxLength value="50"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="carga" type="tpCarga">
							<xs:annotation>
								<xs:documentation>Dados da carga que indica o tipo da viagem, podendo ser lotação ou fracionada.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="contatos" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Contatos a serem adicionados a operação de transporte.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="contato" type="tpContato" maxOccurs="unbounded"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="transp">
							<xs:annotation>
								<xs:documentation>Definições de elementos ligados ao transporte </xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="rntrc" type="tpRntrc">
										<xs:annotation>
											<xs:documentation>Contratado responsável pela prestação de serviço e que receberá os valores do frete.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:choice>
										<xs:element name="cpfTransportador" type="tpCpf">
											<xs:annotation>
												<xs:documentation>CPF do transportador.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="cnpjTransportador" type="tpCnpj">
											<xs:annotation>
												<xs:documentation>CNPJ do transportador.</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:choice>
									<xs:element name="infTransportador" type="tpTransportador" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Informações do transportador.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="gestoraCartao" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Código de identificação da gestora de cartão que será usada para o pagamento desta. Este campo é opcional porque não precisa ser informado quando a operação for sem movimentação financeira para parcelas e pedágio.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="tpGestoraCartao">
												<xs:minLength value="3"/>
												<xs:maxLength value="3"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="subcontratado" type="tpSubcontratado" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Informações referentes ao subcontratado, se existirem.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="rota" type="tpRota" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Define a rora da operação de transporte.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="condutores" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Dados dos condutores</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element name="condutor" maxOccurs="unbounded">
													<xs:annotation>
														<xs:documentation>Dados do condutor</xs:documentation>
													</xs:annotation>
													<xs:complexType>
														<xs:sequence>
															<xs:element name="cpf" type="tpCpf">
																<xs:annotation>
																	<xs:documentation>Número do CPF do condutor.</xs:documentation>
																</xs:annotation>
															</xs:element>
															<xs:element name="informacoes" type="tpCondutor" minOccurs="0">
																<xs:annotation>
																	<xs:documentation>Informações do condutor.</xs:documentation>
																</xs:annotation>
															</xs:element>
														</xs:sequence>
													</xs:complexType>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
									<xs:element name="veiculos">
										<xs:annotation>
											<xs:documentation>Veículos responsáveis pelo transporte.</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element name="veiculo" maxOccurs="5">
													<xs:annotation>
														<xs:documentation>Dados do veículo</xs:documentation>
													</xs:annotation>
													<xs:complexType>
														<xs:sequence>
															<xs:element name="placa">
																<xs:annotation>
																	<xs:documentation>Placa do veículo</xs:documentation>
																</xs:annotation>
																<xs:simpleType>
																	<xs:restriction base="xs:string">
																		<xs:minLength value="1"/>
																		<xs:maxLength value="7"/>
																		<xs:pattern value="[a-zA-Z0-9]+"/>
																	</xs:restriction>
																</xs:simpleType>
															</xs:element>
															<xs:element name="informacoes" type="tpVeiculo" minOccurs="0">
																<xs:annotation>
																	<xs:documentation>Informações do veículo.</xs:documentation>
																</xs:annotation>
															</xs:element>
														</xs:sequence>
													</xs:complexType>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
									<xs:element name="valores" type="tpValores">
										<xs:annotation>
											<xs:documentation>Valores do controle de viagem. Para operações de transporte sem movimentação financeira o desconto será considerado como um desconto de frete e esta valor não alterará o valor informado no frete.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="categoriaPedagio" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Se a rota estiver utilizando o  roteirizador é necessário informar qual a categoria para calculo de pedágio. Valor padrão é 0 = Isento</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="tpCategoriaPedagio">
												<xs:minInclusive value="0"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="quitacao" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Coleção de documentos que deram origem a operação de transporte. Os documentos serão inseridos na parcela de saldo.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="documentos">
										<xs:annotation>
											<xs:documentation>Documentos da quitação.</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element name="documento" type="tpDocumento" maxOccurs="unbounded"/>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
									<xs:element name="entrega" type="tpCnpj" minOccurs="0">
										<xs:annotation>
											<xs:documentation>CNPJ de entrega dos documentos de quitação. Por padrão os docmentos são enviados ao endereço do ponto emissor (emitente).</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="adicionais" type="tpAdicionalImpressao" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Dados adicionais para uso na impressão</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="confirmador" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Informa o confirmador de recebimento da(s) carga(s).</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:choice>
									<xs:element name="cnpj" type="tpCnpj">
										<xs:annotation>
											<xs:documentation>Número do CNPJ.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="cpf" type="tpCpf">
										<xs:annotation>
											<xs:documentation>Número do CPF.</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:choice>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="ID" type="tpGUID" use="optional">
						<xs:annotation>
							<xs:documentation>Identificador único para a operação de transporte que deve se usado para a assinatura digital.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="Id" type="tpGUID" use="optional">
						<xs:annotation>
							<xs:documentation>Identificador único para a operação de transporte que deve se usado para a assinatura digital.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="gerPgtoFin" type="tpSimNao" use="optional">
						<xs:annotation>
							<xs:documentation>Determina se os valores da operação de transporte terão a movimentação financeira ou não. 1 = Sim e 2 = Não. Por padrão existe a movimentação.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="gerPgtoPedagio" type="tpPagamentoPedagio" use="optional">
						<xs:annotation>
							<xs:documentation>Determina a configuração de pagamento do pedágio. 1 = Com movimentação financeira em cartão, 2 = Sem movimentação financeira em cartão. Por padrão, será com movimentação financeira em cartão.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="impAuto" type="tpSimNao" use="optional">
						<xs:annotation>
							<xs:documentation>Determina se deverá ser impressa automaticamente a operação de transporte ou não. 1 = Sim e 2 = Não. Por padrão será impressa automaticamente.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="tipoOp" use="optional">
						<xs:annotation>
							<xs:documentation>Determina o tipo da operação de transporte. Poderão haver operações para várias finalidades como por exemplo: frota própria. Por padrão será assumido que as operações são de frete contratado (1).</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:byte">
								<xs:pattern value="1"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
					<xs:attribute name="utilizaDirecionamentoPedagio" type="tpSimNao" use="optional">
						<xs:annotation>
							<xs:documentation>Determina se a operação de transporte utilizará direcionamento de pedágio por condutor.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element ref="ds:Signature"/>
		</xs:sequence>
	</xs:complexType>
