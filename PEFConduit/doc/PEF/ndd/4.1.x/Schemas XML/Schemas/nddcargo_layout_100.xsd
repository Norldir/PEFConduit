		<xs:sequence>
			<xs:element name="UF">
				<xs:annotation>
					<xs:documentation>Sigla da UF, , informar EX para operações com o exterior.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="tpString">
						<xs:minLength value="2"/>
						<xs:maxLength value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="codigoMunicipio">
				<xs:annotation>
					<xs:documentation>Código do município (utilizar a tabela do IBGE), informar 9999999 para operações com o exterior.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="tpCodigoIBGE"/>
				</xs:simpleType>
			</xs:element>
			<xs:element name="bairro">
				<xs:annotation>
					<xs:documentation>Nome do bairro. Informar EXTERIOR caso seja do exterior.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="tpString">
						<xs:minLength value="1"/>
						<xs:maxLength value="255"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="logradouro">
				<xs:annotation>
					<xs:documentation>Nome do logradouro. Informar EXTERIOR caso seja do exterior.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="tpString">
						<xs:minLength value="1"/>
						<xs:maxLength value="255"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="numero" type="tpString">
				<xs:annotation>
					<xs:documentation>Número. Informar S/N caso seja do exterior.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CEP" type="tpString">
				<xs:annotation>
					<xs:documentation>Número do CEP. Informar 0 caso seja do exterior.</xs:documentation>
					<xs:documentation>Informar zeros não significativos</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="complemento" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Complemento</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="tpString">
						<xs:minLength value="1"/>
						<xs:maxLength value="255"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
