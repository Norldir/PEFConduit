				<xs:simpleType>
					<xs:restriction base="tpString">
						<xs:minLength value="1"/>
						<xs:maxLength value="255"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="endereco" type="tpEndereco">
				<xs:annotation>
					<xs:documentation>Informações sobre o endereço do remetente.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
