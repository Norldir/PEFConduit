			<xs:element name="infOT">
				<xs:annotation>
					<xs:documentation>Informações sobre a operação de transporte.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="mensagem" type="tpMensagem">
							<xs:annotation>
								<xs:documentation>Mensagem de retorno</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="dataHora" type="tpDataHora">
							<xs:annotation>
								<xs:documentation>Data e hora do retorno das informações</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ide" type="tpRetornoIdentificacao">
							<xs:annotation>
								<xs:documentation>Dados para identificação da operação de transporte.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="autorizacao" type="tpAutorizacaoOT" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Dados de autorização para a operação de transporte.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="avisoTransportador" type="tpString" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Aviso ao transportador disponibilizado pela ANTT ou Administradora de meios de pagamento</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="valores" type="tpRetornoValores" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Valores refetente a operação de transporte.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="pedagio" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Dados refetentes ao pedágio.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="tpRetornoPedagio"/>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="tpDocumento">
		<xs:annotation>
			<xs:documentation>Documentos de origem da operação de transporte.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="tipo">
				<xs:annotation>
					<xs:documentation>Tipo de documentto.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="tpString">
						<xs:minLength value="1"/>
						<xs:maxLength value="255"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="obrigatorio" type="tpSimNao" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indica se o documento é obigatório entregar ele para a coleta. 1 é obrigatório e 2 = não. Caso não for informado este campo o valor padrão será 2.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dependencias">
				<xs:annotation>
					<xs:documentation>Lista de dependências do documento em questão.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="dependencia" maxOccurs="unbounded">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="descricao">
										<xs:annotation>
											<xs:documentation>Descrição da dependência do documento.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="tpString">
												<xs:minLength value="1"/>
												<xs:maxLength value="255"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="tpRetornoIdentificacao">
		<xs:annotation>
			<xs:documentation>Define os dados de identificação para o retorno.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="cnpj">
				<xs:annotation>
					<xs:documentation>CNPJ da contratante.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="serie" type="tpSerieOT">
				<xs:annotation>
					<xs:documentation>Número de série da operação de transporte,</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="numero" type="tpNumeroOT">
				<xs:annotation>
					<xs:documentation>Número da operação de transporte.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dataEmissao" type="tpDataHora" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Data e hora da emissão  para a operação de transporte.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="tpValores">
		<xs:annotation>
			<xs:documentation>Informações dos valores da operação de transporte.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="vlrFrete" type="tpDec_1302">
				<xs:annotation>
					<xs:documentation>Valor do frete. Este valor é participante do valor total da operação de transporte a ser paga ao transportador.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="despesas" type="tpDespesas" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Despesas a serem levadas em consideração no pagamento das operações de transporte. Este valor é participante do valor total da operação de transporte a ser paga ao transportador.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="parcelamento" type="tpParcelamento" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Define os dados de parcelamento para pagar a operação de transporte. Opcional para o caso do Contratante não gerir seus pagamentos pelo nddCargo</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="retencoes" type="tpImpostos">
				<xs:annotation>
					<xs:documentation>Retenções de valroes para a operação de transporte.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="descontos" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Descontos a serem adicionados na parcela de saldo.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="desconto" type="tpDescontoParcela" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="vlrCombustivel" type="tpDec_1302Opc" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Valor do combustível. Este valor é participante do valor total da operação de transporte a ser paga ao transportador.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="vlrPedagio" type="tpDec_1302" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Valor de vale pedágio a ser pago ao condutor da operação de transporte.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="tpDespesas">
		<xs:annotation>
			<xs:documentation>Informações de despesas da operação de transporte.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="vlrDespesas" type="tpDec_1302">
				<xs:annotation>
					<xs:documentation>Valor total das despesas. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="descricao">
				<xs:annotation>
					<xs:documentation>Descrição das despesas que deram origem ao valor.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="tpString">
						<xs:minLength value="1"/>
						<xs:maxLength value="2000"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="tpRetornoValores">
		<xs:annotation>
			<xs:documentation>Define os dados de retorno para os valores.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="vlrFrete" type="tpDec_1302">
				<xs:annotation>
					<xs:documentation>Valor referente ao frete.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="vlrDespesas" type="tpDec_1302" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Valor referente as despesas.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="vlrCombustivel" type="tpDec_1302" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Valor referente ao combustível.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="vlrSaldo" type="tpDec_1302">
				<xs:annotation>
					<xs:documentation>Valor referente ao saldo da operação de transporte.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="tpRetornoPedagio">
		<xs:annotation>
			<xs:documentation>Define os dados de retorno relacionados ao pedágio.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="valor" type="tpDec_1302">
				<xs:annotation>
					<xs:documentation>Valor total do pedágio.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="origem">
				<xs:annotation>
					<xs:documentation>Cidade e estado de origem.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="tpString">
						<xs:minLength value="1"/>
						<xs:maxLength value="200"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="destino">
				<xs:annotation>
					<xs:documentation>Cidade e estado de destino.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="tpString">
						<xs:minLength value="1"/>
						<xs:maxLength value="200"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="pracas" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Informações das praças de pedágio.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="praca" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Praça de pedágio.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="nome">
										<xs:annotation>
											<xs:documentation>Nome da praça.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="tpString">
												<xs:minLength value="1"/>
												<xs:maxLength value="200"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="valor" type="tpDec_1302">
										<xs:annotation>
											<xs:documentation>Valor da específico da praça de pedágio.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="localizacao">
										<xs:annotation>
											<xs:documentation>Localização da praça de pedágio.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="tpString">
												<xs:minLength value="1"/>
												<xs:maxLength value="200"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="tpImpostos">
		<xs:annotation>
			<xs:documentation>Define os impostos.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="irrf" type="tpDec_1302">
				<xs:annotation>
					<xs:documentation>Imposto de IRRF.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="inss" type="tpDec_1302">
				<xs:annotation>
					<xs:documentation>Imposto de INSS.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="sest" type="tpDec_1302">
				<xs:annotation>
					<xs:documentation>Imposto de SEST.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="senat" type="tpDec_1302">
				<xs:annotation>
					<xs:documentation>Imposto de SENAT.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="tpRestricaoPagamento">
		<xs:annotation>
			<xs:documentation>Define os dados para a restrição de pagamento.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="campo" type="tpCampoRestricao">
				<xs:annotation>
					<xs:documentation>Define o campo para comparação na restrição. Possibilidades: 1 = CNPJ do posto credenciado ou 2 = Data.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="valor">
				<xs:annotation>
					<xs:documentation>Valor para comparação com o campo respeitando o critério.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="tpString">
						<xs:minLength value="1"/>
						<xs:maxLength value="15"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="criterio" type="tpCriterioRestricao" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Define os critérios para a restrição. Possibilidades: 1 = Igual, 2 = Diferente, 3 = Maior, 4 = Maior ou iguak, 5 = Menor e 6 = Menor ou Igual. Por padrão será utilizado Igual (1).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="conector" type="tpConectorRestricao" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Define o conector. Possibilidades: 1 = E e 2 = OU. Por padrão será utilizado E (1).</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="tpTipoPagamento">
		<xs:annotation>
			<xs:documentation>Define os dados do tipo de pagamento.</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element name="manual">
				<xs:annotation>
					<xs:documentation>Dados do pagamento manual da parcela.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="dataPrevisao" type="tpData">
							<xs:annotation>
								<xs:documentation>Data prevista para o pagamento. Padrão AAAA-MM-DD</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="efetivacao" type="tpEfetivacaoPagamento" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Define o local para a efetivação do pagamento. Possibilidades: 1 = Posto credenciado, 2 = Centro de Triagem, = = Contratante e 4 = Confirmador. Por padrão a efetivação deve acontecer no posto credenciado.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="automatico">
				<xs:annotation>
					<xs:documentation>Dados do pagamento automático da parcela.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="dataHora" type="tpDataHora">
							<xs:annotation>
								<xs:documentation>Data e hora para o pagamento. AAAA-MM--DD HH:MM:SS</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="tpParcelamento">
		<xs:annotation>
			<xs:documentation>Define os dados do parcelamento.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice>
				<xs:element name="regraERP">
					<xs:annotation>
						<xs:documentation>Nome da regra de parcelamento no ERP para a integração.</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="tpString">
							<xs:minLength value="1"/>
							<xs:maxLength value="30"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="informações">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="parcelas">
								<xs:annotation>
									<xs:documentation>Lista de parcelas para pagamento da operação de transporte. As parcelas devem serão definidas para definir a maneira de pagar o valor total (frete + combustivel + despesas - descontos - impostos)</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="parcela" type="tpParcela" maxOccurs="unbounded"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="tpParcela">
		<xs:annotation>
			<xs:documentation>Define os dados de parcla para pagamento.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="nome">
				<xs:annotation>
					<xs:documentation>Nome para apresentação da parcela dentro do sistema.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="tpString">
						<xs:minLength value="1"/>
						<xs:maxLength value="50"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="tipoPgto" type="tpTipoPagamento">
				<xs:annotation>
					<xs:documentation>Define o tipo de pagamento da parcela.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="valorAplicado" type="tpDec_1302">
				<xs:annotation>
					<xs:documentation>Valor aplicado para a parcela. Não levando em consideração os descontos.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="valorReal" type="tpDec_1302" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Valor real para pagamento da parcela. Esta tag deve ser informada quando o valor real é diferente do valor aplicado, neste caso é porque houve descontos nesta parcela. Caso não seja informado, será assumido que é o mesmo valor do aplicado.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="documentos" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Lista de documentos a serem coletados no posto credenciado para realização do registro de passagem.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="documento" type="tpDocumento" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="descontos" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Lista de descontos.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="desconto" type="tpDescontoParcela" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="restricoes" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Lista de restrições para que o pagamento da parcela possa ser realizado.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="restricao" type="tpRestricaoPagamento" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="tpAlterarParcela">
		<xs:annotation>
			<xs:documentation>Dados referente a alteração de parcela.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="nome">
				<xs:annotation>
					<xs:documentation>Nome para apresentação da parcela dentro do sistema.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="tpString">
						<xs:minLength value="1"/>
						<xs:maxLength value="50"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="tipoPgto" type="tpTipoPagamento">
				<xs:annotation>
					<xs:documentation>Define o tipo de pagamento da parcela.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="valorAplicado" type="tpDec_1302">
				<xs:annotation>
					<xs:documentation>Valor aplicado para a parcela. Não levando em consideração os descontos.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="valorReal" type="tpDec_1302" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Valor real para pagamento da parcela. Esta tag deve ser informada quando o valor real é diferente do valor aplicado, neste caso é porque houve descontos nesta parcela. Caso não seja informado, será assumido que é o mesmo valor do aplicado.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="documentos" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Lista de documentos a serem coletados no posto credenciado para realização do registro de passagem.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="documento" type="tpDocumento" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="descontos" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Lista de descontos.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="desconto" type="tpDescontoParcela" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="restricoes" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Lista de restrições para que o pagamento da parcela possa ser realizado.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="restricao" type="tpRestricaoPagamento" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="retencoes" type="tpImpostos" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Valores de retenção.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="rubrica" type="tpRubricaPgto">
				<xs:annotation>
					<xs:documentation>Informar qual rubrica o valor se refere. 1 = Frete, 2 = Combustível e 3 = Despesa</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="tpRetornoPessoaJuridica">
		<xs:annotation>
			<xs:documentation>Dados de retorno do cadastro de pessoa jurídica.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="cnpj" type="tpCnpj">
				<xs:annotation>
					<xs:documentation>CNPJ da pessoa jurídica.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="nome">
				<xs:annotation>
					<xs:documentation>Nome da pessoa juridica.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="tpString">
						<xs:minLength value="1"/>
						<xs:maxLength value="150"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="endereco" type="tpEndereco">
				<xs:annotation>
					<xs:documentation>Endereço da pessoa jurídica.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="tpRetornoPessoa">
		<xs:annotation>
			<xs:documentation>Dados de retorno do cadastro de pessoa.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice>
				<xs:element name="cpf" type="tpCpf">
					<xs:annotation>
						<xs:documentation>CPF da pessoa.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="cnpj" type="tpCnpj">
					<xs:annotation>
						<xs:documentation>CNPJ da pessoa.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:element name="nome">
				<xs:annotation>
					<xs:documentation>Nome da pessoa.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="tpString">
						<xs:minLength value="1"/>
						<xs:maxLength value="150"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="endereco" type="tpEndereco">
				<xs:annotation>
					<xs:documentation>Endereço da pessoa.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="tpAdicionalParcela">
		<xs:annotation>
			<xs:documentation>Dados de adicionais das parcelas.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="valor" type="tpDec_1302">
				<xs:annotation>
					<xs:documentation>Valor a ser acrescido.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="rubrica" type="tpRubricaPgto">
				<xs:annotation>
					<xs:documentation>Informar qual rubrica o valor se refere. 1 = Frete, 2 = Combustível e 3 = Despesa</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="tpDescontoParcela">
		<xs:annotation>
			<xs:documentation>Desconto a ser utilizado para cálculo da parcela.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="tpDesc" type="tpSimNao">
				<xs:annotation>
					<xs:documentation>Tipo de desconto, 1 se for descontado no incio da operação ou 2 se for descontado no registro de passagem.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="nmDesc">
				<xs:annotation>
					<xs:documentation>Nome para o desconto.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="tpString">
						<xs:minLength value="1"/>
						<xs:maxLength value="50"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="vlrDesc" type="tpDec_1302" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Valor do desconto quando retido na fonte</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dsDesc" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Informações adicionaris para o desconto.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="tpString">
						<xs:minLength value="1"/>
						<xs:maxLength value="255"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="tpDescontoParcelaAlteracao">
		<xs:sequence>
			<xs:element name="tpDesc" type="tpSimNao">
				<xs:annotation>
					<xs:documentation>Tipo de desconto, 1 se for descontado no incio da operação ou 2 se for descontado no registro de passagem.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="nmDesc">
				<xs:annotation>
					<xs:documentation>Nome para o desconto.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="tpString">
						<xs:minLength value="1"/>
						<xs:maxLength value="50"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="vlrDesc" type="tpDec_1302" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Valor do desconto quando retido na fonte.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dsDesc" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Informações adicionaris para o desconto.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="tpString">
						<xs:minLength value="1"/>
						<xs:maxLength value="255"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="rubrica" type="tpRubricaPgto" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Informar qual rubrica o valor se refere. 1 = Frete, 2 = Combustível e 3 = Despesa. Por padrão o desconto não é nas rubricas e sim diretamente na parcela.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="tpRota">
		<xs:annotation>
			<xs:documentation>Define os dados da rota.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="rotaERP">
				<xs:annotation>
					<xs:documentation>Nome da rota no ERP para a integração. Caso a tag abaixo seja informada, este será o nome usado para as próximas integrações.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="tpString">
						<xs:minLength value="1"/>
						<xs:maxLength value="30"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="informacoes" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Dados da rota a serem inseridos ou atualizados no sistema. Se a tag acima for identica a alguma rota no banco, as informações deta serão alteradas por esta tag.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="nome">
							<xs:annotation>
								<xs:documentation>Nome para apresentação da rota dentro do sistema.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="tpString">
									<xs:minLength value="1"/>
									<xs:maxLength value="50"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="tipoRotaPadrao" type="tpTipoRota" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Define o tipo de rota padrão a ser utilizado nos pontos de parada que não possuem um tipo específico. Por padrão será utilizado a rota rápida(1).</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="pontosParada">
							<xs:annotation>
								<xs:documentation>Define os pontos de parada, bem como a origem e destino. A sequencia de pontos de parada na lista é a sequencia da rota.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="pontoParada" minOccurs="2" maxOccurs="unbounded">
										<xs:complexType>
											<xs:sequence>
												<xs:choice>
													<xs:element name="codigoIBGE" type="tpCodigoIBGE">
														<xs:annotation>
															<xs:documentation>Código do IBGE do ponto de parada.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="cep">
														<xs:annotation>
															<xs:documentation>CEP do ponto de parada.</xs:documentation>
														</xs:annotation>
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:pattern value="[0-9]{8}"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
												</xs:choice>
												<xs:element name="tipoRotaEspecifico" type="tpTipoRota" minOccurs="0">
													<xs:annotation>
														<xs:documentation>Define o tipo de rota específico para o ponto de parada em relação ao anterior. Isso deve ignorar o tipo de rota definido como padrão.</xs:documentation>
													</xs:annotation>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="responsaveis" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Lista de responsáveis específicos para a rota.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="responsavel" maxOccurs="unbounded">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="nome">
													<xs:annotation>
														<xs:documentation>Nome do responsável.</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="tpString">
															<xs:minLength value="1"/>
															<xs:maxLength value="50"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="email">
													<xs:annotation>
														<xs:documentation>Email do responsável.</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="tpEmail">
															<xs:minLength value="1"/>
															<xs:maxLength value="255"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="utilizarRoteirizador" type="tpSimNao" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Possibilita a utilização do retirizador para cálcular o pedágio e a distância. Por padrão será utilizado o roteirizador (1).</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="totalKm" type="tpDec_1302" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Determina a quilometragem da rota.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="notificarRespContratante" type="tpSimNao" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Possibilita a notificação dos responsáveis da contratante caso o roteirizador identifique alterações na rota. Por padrão serão notificados (1).</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!--Lote de operações de transporte-->
	<xs:complexType name="tpLoteOT">
		<xs:annotation>
			<xs:documentation>Lote de operações de transporte.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="operacoes">
				<xs:annotation>
					<xs:documentation>Lista de operações de transporte a serem processadas.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="OT" type="tpOT" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="versao" type="tpVersao" use="required">
			<xs:annotation>
				<xs:documentation>Versão do documento.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="token" type="tpToken" use="required">
			<xs:annotation>
				<xs:documentation>Token de processamento.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="tpRetornoConsultaLoteOT">
		<xs:annotation>
			<xs:documentation>Retorno da consulta de lote de operações de transporte.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="mensagens" type="tpMensagemErro" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Lista das mensagens de erro.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="operacoes" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="OT" type="tpRetornoOT" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Operação transporte.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!--Cancelamento de operação de transporte-->
	<xs:complexType name="tpCancelamento">
		<xs:annotation>
			<xs:documentation>Cancelamento da operação de transporte.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="infCancOT">
				<xs:annotation>
					<xs:documentation>Informações referentes a operação de transporte a ser cancelada.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="cnpj" type="tpCnpj">
							<xs:annotation>
								<xs:documentation>CNPJ da contratante.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="autorizacao" type="tpAutorizacaoOT">
							<xs:annotation>
								<xs:documentation>Operação de transporte a ser cancelada.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="motivo">
							<xs:annotation>
								<xs:documentation>Motivo pelo qual foi solicitado o cancelamento da operação de transporte.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="tpString">
									<xs:minLength value="1"/>
									<xs:maxLength value="500"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="ID" type="tpGUID" use="required">
						<xs:annotation>
							<xs:documentation>Identificador único para o cancelamento.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element ref="ds:Signature"/>
		</xs:sequence>
		<xs:attribute name="versao" type="tpVersao" use="required">
			<xs:annotation>
				<xs:documentation>Versão do documento.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="token" type="tpToken" use="required">
			<xs:annotation>
				<xs:documentation>Token de processamento.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="tpRetornoCancelamento">
		<xs:annotation>
			<xs:documentation>Informações sobre o cancelamento.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="dataHora" type="tpDataHora">
				<xs:annotation>
					<xs:documentation>Data e hora do cancelamento.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="autorizacao" type="tpAutorizacaoOT"/>
			<xs:element name="protocoloCanc">
				<xs:annotation>
					<xs:documentation>Protocolo de cancelamento disponibilizado pela ANTT.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="tpString">
						<xs:minLength value="15"/>
						<xs:maxLength value="15"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="tpRetornoConsultaCancelamento">
		<xs:annotation>
			<xs:documentation>Retorno do cancelamento da operação de transporte.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="mensagens" type="tpMensagemErro" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Lista das mensagens de erro.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="cancelamento" type="tpRetornoCancelamento" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Dados referentes ao cancelamento.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!--Alteração de operação de transporte-->
	<xs:complexType name="tpAlteracaoOT">
		<xs:annotation>
			<xs:documentation>Alteração de operação de transporte.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="OT" type="tpAlteraOT"/>
		</xs:sequence>
		<xs:attribute name="versao" type="tpVersao" use="required">
			<xs:annotation>
				<xs:documentation>Versão do documento.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="token" type="tpToken" use="required">
			<xs:annotation>
				<xs:documentation>Token de processamento.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="tpAlteraOT">
		<xs:annotation>
			<xs:documentation>Dados necessários para a alteração de uma OT.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="infOT">
				<xs:annotation>
					<xs:documentation>Informações sobre a operação de transporte.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="cnpj" type="tpCnpj">
							<xs:annotation>
								<xs:documentation>CNPJ da contratante.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="autorizacao" type="tpAutorizacaoOT">
							<xs:annotation>
								<xs:documentation>Dados da autorização para a operação de transporte a ser alterada.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="alteracoes">
							<xs:annotation>
								<xs:documentation>Lista de alterações a serem feitas na operação de transporte.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="alteracao" maxOccurs="unbounded">
										<xs:complexType>
											<xs:choice>
												<xs:element name="ANTT">
													<xs:annotation>
														<xs:documentation>Alteração que diz respeito a dados que a ANTT tem conhecimento.</xs:documentation>
													</xs:annotation>
													<xs:complexType>
														<xs:choice>
															<xs:element name="ate24hrs">
																<xs:annotation>
																	<xs:documentation>Prazo inicial para alteraçãos na ANTT.</xs:documentation>
																</xs:annotation>
																<xs:complexType>
																	<xs:sequence>
																		<xs:element name="validade" minOccurs="0">
																			<xs:annotation>
																				<xs:documentation>Dados de identificação da operação de transporte.</xs:documentation>
																			</xs:annotation>
																			<xs:complexType>
																				<xs:sequence>
																					<xs:element name="dtInicio" type="tpData">
																						<xs:annotation>
																							<xs:documentation>Data de inicio da viagem.</xs:documentation>
																						</xs:annotation>
																					</xs:element>
																					<xs:element name="dtFim" type="tpData">
																						<xs:annotation>
																							<xs:documentation>Data de término da viagem.</xs:documentation>
																						</xs:annotation>
																					</xs:element>
																				</xs:sequence>
																			</xs:complexType>
																		</xs:element>
																		<xs:element name="carga" type="tpAlterarCarga" minOccurs="0">
																			<xs:annotation>
																				<xs:documentation>Dados referente a carga da viagem.</xs:documentation>
																			</xs:annotation>
																		</xs:element>
																		<xs:element name="veiculos" minOccurs="0">
																			<xs:annotation>
																				<xs:documentation>Veículos responsáveis pelo transporte. Deve ser enviado toda a lista de veículos.</xs:documentation>
																			</xs:annotation>
																			<xs:complexType>
																				<xs:sequence>
																					<xs:element name="veiculo" maxOccurs="5">
																						<xs:annotation>
																							<xs:documentation>Dados do veículo</xs:documentation>
																						</xs:annotation>
																						<xs:complexType>
																							<xs:sequence>
																								<xs:element name="placa">
																									<xs:annotation>
																										<xs:documentation>Placa do veículo</xs:documentation>
																									</xs:annotation>
																									<xs:simpleType>
																										<xs:restriction base="xs:string">
																											<xs:minLength value="1"/>
																											<xs:maxLength value="7"/>
																											<xs:whiteSpace value="preserve"/>
																											<xs:pattern value="[A-Z0-9]+"/>
																										</xs:restriction>
																									</xs:simpleType>
																								</xs:element>
																							</xs:sequence>
																						</xs:complexType>
																					</xs:element>
																				</xs:sequence>
																			</xs:complexType>
																		</xs:element>
																		<xs:element name="categoriaPedagio" minOccurs="0">
																			<xs:annotation>
																				<xs:documentation>Se a rota estiver utilizando o  roteirizador é necessário informar qual a categoria para calculo de pedágio. Valor padrão é 0 = Isento</xs:documentation>
																			</xs:annotation>
																			<xs:simpleType>
																				<xs:restriction base="tpCategoriaPedagio">
																					<xs:minInclusive value="0"/>
																				</xs:restriction>
																			</xs:simpleType>
																		</xs:element>
																	</xs:sequence>
																</xs:complexType>
															</xs:element>
															<xs:element name="depois24hrs">
																<xs:annotation>
																	<xs:documentation>Prazo final para alterações na ANTT.</xs:documentation>
																</xs:annotation>
																<xs:complexType>
																	<xs:sequence>
																		<xs:element name="veiculos" minOccurs="0">
																			<xs:annotation>
																				<xs:documentation>Veículos responsáveis pelo transporte.</xs:documentation>
																			</xs:annotation>
																			<xs:complexType>
																				<xs:sequence>
																					<xs:element name="veiculo" maxOccurs="5">
																						<xs:annotation>
																							<xs:documentation>Dados do veículo</xs:documentation>
																						</xs:annotation>
																						<xs:complexType>
																							<xs:sequence>
																								<xs:element name="placa">
																									<xs:annotation>
																										<xs:documentation>Placa do veículo</xs:documentation>
																									</xs:annotation>
																									<xs:simpleType>
																										<xs:restriction base="xs:string">
																											<xs:minLength value="1"/>
																											<xs:maxLength value="7"/>
																											<xs:whiteSpace value="preserve"/>
																											<xs:pattern value="[A-Z0-9]+"/>
																										</xs:restriction>
																									</xs:simpleType>
																								</xs:element>
																							</xs:sequence>
																						</xs:complexType>
																					</xs:element>
																				</xs:sequence>
																			</xs:complexType>
																		</xs:element>
																		<xs:element name="categoriaPedagio" minOccurs="0">
																			<xs:annotation>
																				<xs:documentation>Se a rota estiver utilizando o  roteirizador é necessário informar qual a categoria para calculo de pedágio. Valor padrão é 0 = Isento</xs:documentation>
																			</xs:annotation>
																			<xs:simpleType>
																				<xs:restriction base="tpCategoriaPedagio">
																					<xs:minInclusive value="0"/>
																				</xs:restriction>
																			</xs:simpleType>
																		</xs:element>
																	</xs:sequence>
																</xs:complexType>
															</xs:element>
														</xs:choice>
													</xs:complexType>
												</xs:element>
												<xs:element name="financeiro">
													<xs:annotation>
														<xs:documentation>Alteração em dados que envolvem o fluxo financeiro.</xs:documentation>
													</xs:annotation>
													<xs:complexType>
														<xs:sequence>
															<xs:choice>
																<xs:element name="novaParc" type="tpAlterarParcela">
																	<xs:annotation>
																		<xs:documentation>Inclusão de nova parcela para a operação de transporte.</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="ajustaParc">
																	<xs:annotation>
																		<xs:documentation>Ajustar parcela existente na operação de transporte.</xs:documentation>
																	</xs:annotation>
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element name="nome">
																				<xs:annotation>
																					<xs:documentation>Nome da parcela ja existente na operação de transporte.</xs:documentation>
																				</xs:annotation>
																				<xs:simpleType>
																					<xs:restriction base="tpString">
																						<xs:minLength value="1"/>
																						<xs:maxLength value="50"/>
																					</xs:restriction>
																				</xs:simpleType>
																			</xs:element>
																			<xs:choice>
																				<xs:element name="data" type="tpDataHora">
																					<xs:annotation>
																						<xs:documentation>Novo valor de data, se a parcela for automativa o agendamente será alterado e caso seja manual a data prevista será alterada.</xs:documentation>
																					</xs:annotation>
																				</xs:element>
																				<xs:element name="valores">
																					<xs:annotation>
																						<xs:documentation>Ajuste nos valores da parcela.</xs:documentation>
																					</xs:annotation>
																					<xs:complexType>
																						<xs:sequence>
																							<xs:choice>
																								<xs:element name="adicional" type="tpAdicionalParcela">
																									<xs:annotation>
																										<xs:documentation>Incluir um valor adicional para a parcela.</xs:documentation>
																									</xs:annotation>
																								</xs:element>
																								<xs:element name="desconto" type="tpDescontoParcelaAlteracao">
																									<xs:annotation>
																										<xs:documentation>Incluir um desconto de valor para a parcela.</xs:documentation>
																									</xs:annotation>
																								</xs:element>
																								<xs:element name="retencoes" type="tpImpostos">
																									<xs:annotation>
																										<xs:documentation>Valores de retenções.</xs:documentation>
																									</xs:annotation>
																								</xs:element>
																							</xs:choice>
																						</xs:sequence>
																					</xs:complexType>
																				</xs:element>
																			</xs:choice>
																		</xs:sequence>
																	</xs:complexType>
																</xs:element>
																<xs:element name="pedagio">
																	<xs:annotation>
																		<xs:documentation>Atualizar o valor de pedagio da operação de transporte.</xs:documentation>
																	</xs:annotation>
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element name="valor" type="tpDec_1302">
																				<xs:annotation>
																					<xs:documentation>Informar o novo valor total do pedágio.</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																		</xs:sequence>
																	</xs:complexType>
																</xs:element>
															</xs:choice>
														</xs:sequence>
													</xs:complexType>
												</xs:element>
											</xs:choice>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="motivo">
							<xs:annotation>
								<xs:documentation>Descrever o motivo da alteração.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="tpString">
									<xs:minLength value="1"/>
									<xs:maxLength value="1000"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="ID" type="tpGUID" use="required">
						<xs:annotation>
							<xs:documentation>Identificador único para a operação de transporte que deve se usado para a assinatura digital.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element ref="ds:Signature"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="tpRetornoAlteracao">
		<xs:annotation>
			<xs:documentation>Retorno da alteração de operação de transporte.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="mensagens" type="tpMensagemErro" minOccurs="0"/>
			<xs:element name="alteracao" type="tpRetornoConsultaAlteracao" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="tpRetornoConsultaAlteracao">
		<xs:annotation>
			<xs:documentation>Dados de retorno da alteração.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="dataHora" type="tpDataHora">
				<xs:annotation>
					<xs:documentation>Data e hora da alteração.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="autorizacao" type="tpAutorizacaoOT">
				<xs:annotation>
					<xs:documentation>Identificação da operação de transporte.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ide" type="tpRetornoIdentificacao">
				<xs:annotation>
					<xs:documentation>Dados para identificação da operação de transporte.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="valores" type="tpRetornoValores">
				<xs:annotation>
					<xs:documentation>Valores refetente a operação de transporte.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="pedagio" type="tpRetornoPedagio" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<!--Integração dos pagamentos realizados-->
	<xs:complexType name="tpConsultaPgtos">
		<xs:annotation>
			<xs:documentation>Dados necessários para a consulta de pagamento realizados.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice>
				<xs:element name="cnpjContratante" type="tpCnpj">
					<xs:annotation>
						<xs:documentation>CNPJ da contratante solicitante dos pagamentos.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="cnpjEmitente" type="tpCnpj">
					<xs:annotation>
						<xs:documentation>CNPJ do emitente solicitante dos pagamentos.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:choice>
				<xs:element name="GUID" type="tpGUID">
					<xs:annotation>
						<xs:documentation>Identificador único a ser associado com a integração de pagamentos. Caso ja existe uma integração para este identificador, esta será retornada.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="periodo">
					<xs:annotation>
						<xs:documentation>Intervalo de datas a ser retornado os pagamentos.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="dataInicial" type="tpData">
								<xs:annotation>
									<xs:documentation>Data inicial para a conuslta de pagamentos.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="dataFinal" type="tpData">
								<xs:annotation>
									<xs:documentation>Data final para a consulta de pagamentos.</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:sequence>
		<xs:attribute name="versao" type="tpVersao" use="required">
			<xs:annotation>
				<xs:documentation>Versão do documento.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="token" type="tpToken" use="required">
			<xs:annotation>
				<xs:documentation>Token de processamento.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="tpRetornoConsultaPgtos">
		<xs:annotation>
			<xs:documentation>Retorno das consultas de pagementos realizados.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="mensagens" type="tpMensagemErro" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Lista das mensagens de erro.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="pgtos" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Pagamentos realizados.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="pgto" type="tpPgto" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="tpPgto">
		<xs:annotation>
			<xs:documentation>Dados de pagamento realizado.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="autorizacao" type="tpAutorizacaoOT">
				<xs:annotation>
					<xs:documentation>Identificação da operação de transporte.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dataHora" type="tpDataHora">
				<xs:annotation>
					<xs:documentation>Data e hora que o pagamento foi realizado.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:choice>
				<xs:element name="pedagio">
					<xs:annotation>
						<xs:documentation>Pagamento específico de pedágio.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="valorPago" type="tpDec_1302">
								<xs:annotation>
									<xs:documentation>Valor total do pagamento de pedágio.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="estabelecimento">
								<xs:annotation>
									<xs:documentation>Estabelecimento onde foi pago o valor do pedágio.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="cnpj" type="tpCnpj">
											<xs:annotation>
												<xs:documentation>CNPJ do estabelecimento.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="nome">
											<xs:annotation>
												<xs:documentation>Nome do estabelecimento.</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="tpString">
													<xs:minLength value="1"/>
													<xs:maxLength value="150"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="endereco" type="tpEndereco">
											<xs:annotation>
												<xs:documentation>Endereço do estabelecimento.</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="frete">
					<xs:annotation>
						<xs:documentation>Pagamento específico do frete, levando em consideração o combustível e também as despesas.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="parcelas">
								<xs:annotation>
									<xs:documentation>Parcelas do CIOT envolvidas no pagamento.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="parcela" maxOccurs="unbounded">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="nome">
														<xs:annotation>
															<xs:documentation>Nome da parcela.</xs:documentation>
														</xs:annotation>
														<xs:simpleType>
															<xs:restriction base="tpString">
																<xs:minLength value="1"/>
																<xs:maxLength value="50"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="valorPago" type="tpDec_1302">
														<xs:annotation>
															<xs:documentation>Valor do pagamento realizado</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="efetivacao">
														<xs:annotation>
															<xs:documentation>Local da efetivação do pagamento.</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:choice>
																	<xs:element name="postoCredenciado" type="tpRetornoPessoaJuridica">
																		<xs:annotation>
																			<xs:documentation>Pagamento realizado devido a um registro de passagem no posto credenciado.</xs:documentation>
																		</xs:annotation>
																	</xs:element>
																	<xs:element name="centroTriagem" type="tpRetornoPessoaJuridica">
																		<xs:annotation>
																			<xs:documentation>Pagamento realizado devido a uma triagem de coleta com sucesso no centro de triagem.</xs:documentation>
																		</xs:annotation>
																	</xs:element>
																	<xs:element name="contratante" type="tpRetornoPessoaJuridica">
																		<xs:annotation>
																			<xs:documentation>Pagamento realizado devido a uma triagem de coleta com sucesso na contratante.</xs:documentation>
																		</xs:annotation>
																	</xs:element>
																	<xs:element name="confirmacaoEletronica" type="tpRetornoPessoa">
																		<xs:annotation>
																			<xs:documentation>Pagamento realizado devido a uma confirmação eletrônica de recebimento da carga.</xs:documentation>
																		</xs:annotation>
																	</xs:element>
																	<xs:element name="automatica">
																		<xs:annotation>
																			<xs:documentation>Pagamento realizado devido a um agendamento.</xs:documentation>
																		</xs:annotation>
																		<xs:complexType>
																			<xs:sequence>
																				<xs:element name="dataHora" type="tpDataHora">
																					<xs:annotation>
																						<xs:documentation>Data e hora do agendamento.</xs:documentation>
																					</xs:annotation>
																				</xs:element>
																			</xs:sequence>
																		</xs:complexType>
																	</xs:element>
																</xs:choice>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="comentario" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Comentário realizado durante a ação que deu origem ao pagamento.</xs:documentation>
														</xs:annotation>
														<xs:simpleType>
															<xs:restriction base="tpString">
																<xs:minLength value="1"/>
																<xs:maxLength value="500"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<!--Solicitação de impressão da operação de transporte-->
	<xs:complexType name="tpSolicitarImpressao">
		<xs:annotation>
			<xs:documentation>Solicitar a impressão de uma operação de transporte.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="cnpj" type="tpCnpj">
				<xs:annotation>
					<xs:documentation>CNPJ da contratante.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ciot" type="tpCIOT">
				<xs:annotation>
					<xs:documentation>Código identificador da operação de transporte.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ptEmissor">
				<xs:annotation>
					<xs:documentation>Ponto emissor para realizar a impressão.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="tpString">
						<xs:minLength value="1"/>
						<xs:maxLength value="30"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="versao" type="tpVersao" use="required">
			<xs:annotation>
				<xs:documentation>Versão do documento.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="token" type="tpToken" use="required">
			<xs:annotation>
				<xs:documentation>Token de processamento.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<!--Cross Talk Message-->
	<xs:complexType name="CrossTalk_Header_Response_Type">
		<xs:annotation>
			<xs:documentation>Cabeçalho da mensagem</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ResponseCode" type="xs:int">
				<xs:annotation>
					<xs:documentation>Codigo da resposta do processamento da mensagem</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ResponseCodeMessage" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Descricao do codigo da mensagem</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ProcessCode" type="xs:int">
				<xs:annotation>
					<xs:documentation>Código do processamento</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="MessageType" type="xs:int">
				<xs:annotation>
					<xs:documentation>Tipo de mensagem que será processada</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ExchangePattern" type="xs:byte" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Padrão de troca de mensagem</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SourceId" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Identifdicador do sistema que está ntegrando</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:byte">
						<xs:pattern value="0|1|2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="GUID" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Identificador unico gerado pela aplicação para poder recuperar a mensagem </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DateTime" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Data e hora do envio da mensagem para processamento</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EnterpriseId" type="xs:long">
				<xs:annotation>
					<xs:documentation>Identificador da empresa</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Token" type="xs:string">
				<xs:annotation>
					<xs:documentation>Identificador exclusivo que contêm os vinculos de processamento necessários no ETransportBusiness</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ContentType" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Tipo de conteudo da mensagem enviada</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ContentEncoding" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Tipo de codificação dos caracteres passados na mensagem</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
